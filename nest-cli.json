{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*",
        "outDir": "dist/keys",
        "input": "keys"
      }
    ],
    "watchAssets": true
  }
}
